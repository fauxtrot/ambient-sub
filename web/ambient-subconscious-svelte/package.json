{
  "name": "ambient-subconscious",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "dev:bridge": "tsx process-bridge-server.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:bridge\"",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "generate": "spacetime generate --out-dir src/generated --lang typescript"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/node": "^25.0.10",
    "@types/ws": "^8.5.13",
    "concurrently": "^9.1.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  },
  "dependencies": {
    "spacetimedb": "^1.11.3",
    "wavesurfer.js": "^7.12.1",
    "ws": "^8.19.0"
  },
  "type": "module"
}
